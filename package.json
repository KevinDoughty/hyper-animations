    "babel-generator": "^6.26.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-tester": "^4.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-template": "^6.26.0",
    "babel-traverse": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0"
  },
